<launch>
 <arg name = "semantic_files" value = "$(find adaptive_controller)/asp/semantic_map.lp" />

 <node pkg="adaptive_controller" type="irun.py" name="rosoclingo_controller" args="$(arg semantic_files)" output="screen" required="true"/>

 <node pkg="adaptive_controller" type="actionsFulfiller.py" name="action_fulfiller" output="screen" respawn="true"/>

 <node pkg="adaptive_controller" type="minimalRequest.py" output="screen" name="task_requester"/>

</launch>
